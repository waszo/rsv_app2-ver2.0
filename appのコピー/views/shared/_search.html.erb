<div class="search-form">
  <%= search_form_for @search, url: search_rooms_path do |f| %>
    <%= f.label :adress_cont, 'エリア' %>
    <%= f.search_field :adress_cont, id: "q_adress_cont" %>

    <%= f.label :name_or_description_cont, 'キーワード' %>
    <%= f.search_field :name_or_description_cont, id: "q_name_or_description_cont" %>

    <%= f.submit '検索' %>
  <% end %>
</div>